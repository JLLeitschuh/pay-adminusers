{
  "consumer": {
    "name": "Selfservice-login-attempts"
  },
  "provider": {
    "name": "AdminUsers"
  },
  "interactions": [
    {
      "description": "a valid login attempts update request",
      "provider_state": "a user exists",
      "request": {
        "method": "POST",
        "path": "/v1/api/users/existing-user/attempt-login",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "description": "a valid login attempts reset request",
      "provider_state": "a user exists",
      "request": {
        "method": "POST",
        "path": "/v1/api/users/existing-user/attempt-login",
        "query": "action=reset",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "description": "a valid login attempts update request",
      "provider_state": "a user exists with max login attempts",
      "request": {
        "method": "POST",
        "path": "/v1/api/users/user-login-attempts-max/attempt-login",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 401,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "errors": [
            "invalid username and/or password"
          ]
        },
        "matchingRules": {
          "$.body.errors[0]": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a valid login attempts update request",
      "provider_state": "a user does not exist",
      "request": {
        "method": "POST",
        "path": "/v1/api/users/non-existent-username/attempt-login",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 404,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecificationVersion": "2.0.0"
  }
}